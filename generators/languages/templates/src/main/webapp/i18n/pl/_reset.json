{
    "reset": {
        "request": {
            "title": "Zresetuj swoje hasło",
            "form": {
                "button": "Reset hasła"
            },
            "messages": {
                "info": "Wpisz email użyty podczas rejestracji.",
                "success": "Sprawdź email w celu uzyskania szczegołów jak zresetować hasło.",
                "notfound": "<strong>Email nie był zarejestrowany!</strong> Proszę sprawdź i spróbuj ponownie."
            }
        },
        "finish" : {
            "title": "Reset hasła",
            "form": {
                "button": "Sprawdź nowe hasło"
            },
            "messages": {
                "info": "Wybierz nowe hasło",
                <%_ if (clientFramework === 'angular1') { _%>
                "success": "<strong>Twoje hasło zostało zresetowane.</strong> Proszę <a class=\"alert-link\" href=\"\" ng-click=\"vm.login()\">zaloguj się</a>.",
                <%_ } else { _%>
                "success": "<strong>Twoje hasło zostało zresetowane.</strong> Proszę ",
                <%_ } _%>
                "keymissing": "Brakuje klucza resetującego hasło.",
                "error": "Twoje hasło nie mogło zostać zresetowane. Pamiętaj, że prośba o zresetowanie hasła jest ważna tylko przez 24 godziny."
            }
        }
    }
}
