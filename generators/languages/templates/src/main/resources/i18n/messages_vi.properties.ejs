# Error page
error.title=Không thể thực thi yêu cầu của bạn
error.subtitle=Rất tiếc, có lỗi xảy ra.
error.status=Trạng thái:
error.message=Tin nhắn:

# Activation email
email.activation.title=<%= baseName %> kích hoạt tài khoản
email.activation.greeting=Gửi {0}
email.activation.text1=Tài khoản của bạn <%= baseName %> đã được tạo, vui lòng bấm vào đường dẫn bên dưới để kích hoạt:
email.activation.text2=Trân trọng,
email.signature=Nhóm <%= baseName %>.

# Creation email
email.creation.text1=Tài khoản của bạn <%= baseName %> đã được tạo, vui lòng bấm vào đường dẫn bên dưới để truy cập:

# Reset email
email.reset.title=<%= baseName %> đặt lại mật khẩu
email.reset.greeting=Gửi {0}
email.reset.text1=Để đặt lại mật khẩu cho tài khoản cho tài khoản của bạn <%= baseName %>, vui lòng bấm vào đường dẫn bên dưới:
email.reset.text2=Trân trọng,

<%_ if (enableSocialSignIn) { _%>
# Social validation email
email.social.registration.title=<%= baseName %> kích hoạt tài khoản
email.social.registration.greeting=Gửi {0}
email.social.registration.text1=Tài khoản của bạn <%= baseName %> đã được tạo từ tài khoản {0}.
email.social.registration.text2=Trân trọng,
<%_ } _%>
