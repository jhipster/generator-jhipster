<% let helpBlocks = 0; %> 
{
    "<%= frontendAppName %>": {
        "<%= entityTranslationKey %>" : {
            "home": {
                "title": "<%= entityClassPluralHumanized  %>",
                "refreshListLabel": "പട്ടിക പുതുക്കുക",
                <%_ if (!readOnly) { _%>
                "createLabel": "പുതിയ <%= entityClassHumanized %> സൃഷ്ടിക്കുക",
                "createOrEditLabel": "<%= entityClassHumanized %> സൃഷ്ടിക്കുക അല്ലെങ്കിൽ തിരുത്തുക",
                <%_ } _%>
                <%_ if (searchEngineAny) { _%>
                "search": "<%= entityClassHumanized %> തിരയുക",
                <%_ } _%>
                "notFound": "ഒന്നും <%= entityClassPluralHumanized %> കണ്ടില്ല"
            },
            <%_ if (!microserviceAppName && !readOnly) { _%>
            "created": "<%= entityClassHumanized %> [identifier: {{ param }}] സൃഷ്ടിച്ചു",
            "updated": "<%= entityClassHumanized %> [identifier: {{ param }}] പുതുക്കി",
            "deleted": "<%= entityClassHumanized %> [identifier: {{ param }}] നീക്കിയിരിക്കുന്നു",
            <%_ } _%>
            <%_ if (!readOnly) { _%>
            "delete": {
                "question": "<%= entityClassHumanized %> [id: {{ id }}] നീക്കണോ എന്ന് സ്ഥിരീകരിക്കുക?"
            },
            <%_ } _%>
            "detail": {
                "title": "<%= entityClassHumanized %>"
            }<% for (idx in fields) {
                if (typeof fields[idx].documentation !== 'undefined') ++helpBlocks; %>,
            "<%= fields[idx].fieldName %>": "<%= fields[idx].fieldNameHumanized %>"<% } %><% for (idx in relationships) { %>,
            "<%= relationships[idx].relationshipName %>": "<%= relationships[idx].relationshipNameHumanized %>"<% } if (helpBlocks > 0) { %>,
            "help": {<% for (idx in fields) {
                if (fields[idx].documentation) {
                    --helpBlocks; %>
                "<%= fields[idx].fieldName %>": "<%= fields[idx].documentation %>"<% if (helpBlocks > 0) { %>,<% }
                } %>
            }
            <% } %>
        }
    }
    <%_ if (microserviceAppName && !readOnly) { _%>,
    "<%= microserviceAppName %>": {
        "<%= entityTranslationKey %>" : {
            "created": "<%= entityClassHumanized %> [identifier: {{ param }}] സൃഷ്ടിച്ചു",
            "updated": "<%= entityClassHumanized %> [identifier: {{ param }}] പുതുക്കി",
            "deleted": "<%= entityClassHumanized %> [identifier: {{ param }}] നീക്കിയിരിക്കുന്നു"
        }
    }
    <%_ } _%>
}
