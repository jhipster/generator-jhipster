    id "org.openapi.generator"
    <%_ } _%>
    <%_ if (!skipClient) { _%>
    id "com.github.node-gradle.node"
    <%_ } _%>
    <%_ if (databaseType === 'sql' && !reactive) { _%>
    id "org.liquibase.gradle"
    <%_ } _%>
    id "org.sonarqube"
    id "io.spring.nohttp"
