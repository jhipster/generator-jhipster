            <groupId>org.hibernate</groupId>
            <artifactId>hibernate-infinispan</artifactId>
            <version>${infinispan-hibernate52.version}</version>
        </dependency>
        <%_ } _%>
        <%_ if (hibernateCache === 'infinispan') { _%>
