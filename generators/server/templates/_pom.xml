            <groupId>com.hazelcast</groupId>
            <artifactId>hazelcast-wm</artifactId>
            <version>${hazelcast-wm.version}</version>
        </dependency>
        <%_ } _%>
        <%_ if (hibernateCache === 'infinispan') { _%>
