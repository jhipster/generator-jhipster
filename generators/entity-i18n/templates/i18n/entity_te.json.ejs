let helpBlocks = 0; %>
{
    "<%= angularAppName %>": {
        "<%= entityTranslationKey %>" : {
            "home": {
                "title": "<%= entityClassPluralHumanized %>",
                "createLabel": "కొత్త <%= entityClassHumanized %>  సృష్టించండి",
                "createOrEditLabel": "<%= entityClassHumanized %> సృష్టించండి లేదా సవరించండి"<% if (searchEngine === 'elasticsearch') { %>,
                "search": "<%= entityClassHumanized %> కొరకు శోధించండి"<% } %>
            },<% if (!microserviceAppName) { %>
            "created": "{{ param }} ఐడీ తో ఒక కొత్త <%= entityClassHumanized %> సృష్టించబడినది",
            "updated": "{{ param }} ఐడీ తో ఒక కొత్త <%= entityClassHumanized %> సవరించబడినది",
            "deleted": "{{ param }} ఐడీ తో ఒక కొత్త <%= entityClassHumanized %> తొలగించబడినది",<% } %>
            "delete": {
                "question": "మీరు <%= entityClassHumanized %> {{ id }} ఖచ్చితంగా తొలగించాలనుకుంటున్నారా?"
            },
            "detail": {
                "title": "<%= entityClassHumanized %>"
            }<% for (idx in fields) {
            if (typeof fields[idx].javadoc !== 'undefined') ++helpBlocks; %>,
            "<%=fields[idx].fieldName%>": "<%= fields[idx].fieldNameHumanized %>"<% } %><% for (idx in relationships) { %>,
            "<%=relationships[idx].relationshipName%>": "<%= relationships[idx].relationshipNameHumanized %>"<% } if (helpBlocks > 0){%>,
            "help": {<% for (idx in fields) {
                if (fields[idx].javadoc) {
                    --helpBlocks; %>
                "<%=fields[idx].fieldName%>": "<%= fields[idx].javadoc %>"<% if (helpBlocks > 0) { %>,<%}
                }
            } %>
            }<% } %>
        }
    }<% if (microserviceAppName) { %>,
    "<%= microserviceAppName %>": {
        "<%= entityTranslationKey %>" : {
            "created": "{{ param }} ఐడీ తో ఒక కొత్త <%= entityClassHumanized %> సృష్టించబడినది",
            "updated": "{{ param }} ఐడీ తో ఒక కొత్త <%= entityClassHumanized %> సవరించబడినది",
            "deleted": "{{ param }} ఐడీ తో ఒక కొత్త <%= entityClassHumanized %> తొలగించబడినది"
        }
    }<% } %>
}
