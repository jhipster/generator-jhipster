let helpBlocks = 0; %>
{
    "<%= frontendAppName %>": {
        "<%= entityTranslationKey %>" : {
            "home": {
                "title": "<%= entityClassPluralHumanized %>",
                "refreshListLabel": "Refresh list",
                "createLabel": "Thêm mới một <%= entityClassHumanized %>",
                "createOrEditLabel": "Thêm mới hoặc cập nhật <%= entityClassHumanized %>"<% if (searchEngine !== false) { %>,
                "search": "Tìm <%= entityClassHumanized %>"<% } %>,
                "notFound": "No <%= entityClassPluralHumanized %> found"
            },<% if (!microserviceAppName) { %>
            "created": "Một <%= entityClassHumanized %> mới được tạo với định danh {{ param }}",
            "updated": "Một <%= entityClassHumanized %> cập nhật với định danh {{ param }}",
            "deleted": "Một <%= entityClassHumanized %> đã xóa với định danh {{ param }}",<% } %>
            "delete": {
                "question": "Bạn có chắc là muốn xóa <%= entityClassHumanized %> {{ id }}?"
            },
            "detail": {
                "title": "<%= entityClassHumanized %>"
            }<% for (idx in fields) {
            if (typeof fields[idx].javadoc !== 'undefined') ++helpBlocks; %>,
            "<%= fields[idx].fieldName %>": "<%= fields[idx].fieldNameHumanized %>"<% } %><% for (idx in relationships) { %>,
            "<%= relationships[idx].relationshipName %>": "<%= relationships[idx].relationshipNameHumanized %>"<% } if (helpBlocks > 0) { %>,
            "help": {<% for (idx in fields) {
                if (fields[idx].javadoc) {
                    --helpBlocks; %>
                "<%= fields[idx].fieldName %>": "<%= fields[idx].javadoc %>"<% if (helpBlocks > 0) { %>,<% }
                }
            } %>
            }<% } %>
        }
    }<% if (microserviceAppName) { %>,
    "<%= microserviceAppName %>": {
        "<%= entityTranslationKey %>" : {
            "created": "Một <%= entityClassHumanized %> mới được tạo với định danh {{ param }}",
            "updated": "Một <%= entityClassHumanized %> cập nhật với định danh {{ param }}",
            "deleted": "Một <%= entityClassHumanized %> đã xóa với định danh {{ param }}"
        }
    }<% } %>
}
