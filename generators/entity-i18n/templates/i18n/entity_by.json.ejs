-%><%
let helpBlocks = 0; %>
{
    "<%= frontendAppName %>": {
        "<%= entityTranslationKey %>" : {
            "home": {
                "title": "<%= entityClassPluralHumanized  %>",
                "refreshListLabel": "Refresh list",
                "createLabel": "Стварыць новы <%= entityClassHumanized %>",
                "createOrEditLabel": "Стварыць або адрэдагаваць <%= entityClassHumanized %>"<% if (searchEngine !== false) { %>,
                "search": "Знайсці <%= entityClassHumanized %>"<% } %>,
                "notFound": "No <%= entityClassPluralHumanized %> found"
            },<% if (!microserviceAppName) { %>
            "created": "Новы <%= entityClassHumanized %> створан з ідэнтыфікатарам {{ param }}",
            "updated": "<%= entityClassHumanized %> абноўлен з ідэнтыфікатарам {{ param }}",
            "deleted": "<%= entityClassHumanized %> выдален з ідэнтыфікатарам {{ param }}",<% } %>
            "delete": {
                "question": "Вы ўпэўнены, што жадаеце выдаліць <%= entityClassHumanized %> {{ id }}?"
            },
            "detail": {
                "title": "<%= entityClassHumanized %>"
            }<% for (idx in fields) {
            if (typeof fields[idx].javadoc !== 'undefined') ++helpBlocks; %>,
            "<%= fields[idx].fieldName %>": "<%= fields[idx].fieldNameHumanized %>"<% } %><% for (idx in relationships) { %>,
            "<%= relationships[idx].relationshipName %>": "<%= relationships[idx].relationshipNameHumanized %>"<% } if (helpBlocks > 0) { %>,
            "help": {<% for (idx in fields) {
                if (fields[idx].javadoc) {
                    --helpBlocks; %>
                "<%= fields[idx].fieldName %>": "<%= fields[idx].javadoc %>"<% if (helpBlocks > 0) { %>,<% }
                }
            } %>
            }<% } %>
        }
    }<% if (microserviceAppName) { %>,
    "<%= microserviceAppName %>": {
        "<%= entityTranslationKey %>" : {
            "created": "Новый <%= entityClassHumanized %> створан з ідэнтыфікатарам {{ param }}",
            "updated": "<%= entityClassHumanized %> абноўлен з ідэнтыфікатарам {{ param }}",
            "deleted": "<%= entityClassHumanized %> выдален з ідэнтыфікатарам {{ param }}"
        }
    }<% } %>
}
