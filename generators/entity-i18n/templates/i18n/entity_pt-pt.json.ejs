-%><%
let helpBlocks = 0; %>
{
    "<%= frontendAppName %>": {
        "<%= entityTranslationKey %>" : {
            "home": {
                "title": "<%= entityClassPluralHumanized  %>",
                "refreshListLabel": "Refresh list",
<%_ if (!readOnly) { _%>
                "createLabel": "Criar novo(a) <%= entityClassHumanized %>",
                "createOrEditLabel": "Criar ou editar <%= entityClassHumanized %>",
<%_ } _%>
<%_ if (searchEngine) { _%>
                "search": "Procurar por <%= entityClassHumanized %>",
<%_ } _%>
                "notFound": "Nenhum <%= entityClassPluralHumanized %> encontrado"
            },
<%_ if (!microserviceAppName && !readOnly) { _%>
            "created": "Um novo <%= entityClassHumanized %> foi criado com o identificador {{ param }}",
            "updated": "<%= entityClassHumanized %> com identificador {{ param }} foi actualizado",
            "deleted": "<%= entityClassHumanized %> com identificador {{ param }} foi eliminado",
<%_ } _%>
<%_ if (!readOnly) { _%>
            "delete": {
                "question": "Tem a certeza que pretende eliminar o(a) <%= entityClassHumanized %> {{ id }}?"
            },
<%_ } _%>
            "detail": {
                "title": "<%= entityClassHumanized %>"
            }<% for (idx in fields) {
            if (typeof fields[idx].javadoc !== 'undefined') ++helpBlocks; %>,
            "<%= fields[idx].fieldName %>": "<%= fields[idx].fieldNameHumanized %>"<% } %><% for (idx in relationships) { %>,
            "<%= relationships[idx].relationshipName %>": "<%= relationships[idx].relationshipNameHumanized %>"<% } if (helpBlocks > 0) { %>,
            "help": {<% for (idx in fields) {
                if (fields[idx].javadoc) {
                    --helpBlocks; %>
                "<%= fields[idx].fieldName %>": "<%= fields[idx].javadoc %>"<% if (helpBlocks > 0) { %>,<% }
                }
            } %>
            }<% } %>
        }
    }
<%_ if (microserviceAppName && !readOnly) { _%>,
    "<%= microserviceAppName %>": {
        "<%= entityTranslationKey %>" : {
            "created": "Um novo <%= entityClassHumanized %> foi criado com o identificador {{ param }}",
            "updated": "<%= entityClassHumanized %> com identificador {{ param }} foi actualizado",
            "deleted": "<%= entityClassHumanized %> com identificador {{ param }} foi eliminado"
        }
    }
<%_ } _%>
}
