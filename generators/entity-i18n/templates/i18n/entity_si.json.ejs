-%><%
let helpBlocks = 0; %>
{
    "<%= frontendAppName %>": {
        "<%= entityTranslationKey %>" : {
            "home": {
                "title": "<%= entityClassPluralHumanized %>",
                "refreshListLabel": "Refresh list",
<%_ if (!readOnly) { _%>
                "createLabel": "නව <%= entityClassHumanized %> එකක් සාදන්න",
                "createOrEditLabel": "<%= entityClassHumanized %> සාදන්න හෝ සංස්කරණය කරන්න",
<%_ } _%>
<%_ if (searchEngine) { _%>
                "search": "<%= entityClassHumanized %> සඳහා සොයන්න",
<%_ } _%>
                "notFound": "<%= entityClassPluralHumanized %> හමු නොවීය"
            },
<%_ if (!microserviceAppName && !readOnly) { _%>
            "created": "{{ param }} හඳුනාගැනීමෙන් නව <%= entityClassHumanized %> සාදනු ලැබේ",
            "updated": "{{ param }} හඳුනාගැනීමෙන් <%= entityClassHumanized %> යාවත්කාලීන වේ",
            "deleted": "{{ param }} හඳුනාගැනීමෙන් <%= entityClassHumanized %> මකා දමනු ලැබේ",
<%_ } _%>
<%_ if (!readOnly) { _%>
            "delete": {
                "question": "<%= entityClassHumanized %> {{ id }} මකා දැමීමට ඔබට විශ්වාසද?"
            },
<%_ } _%>
            "detail": {
                "title": "<%= entityClassHumanized %>"
            }<% for (idx in fields) {
            if (typeof fields[idx].javadoc !== 'undefined') ++helpBlocks; %>,
            "<%= fields[idx].fieldName %>": "<%= fields[idx].fieldNameHumanized %>"<% } %><% for (idx in relationships) { %>,
            "<%= relationships[idx].relationshipName %>": "<%= relationships[idx].relationshipNameHumanized %>"<% } if (helpBlocks > 0) { %>,
            "help": {<% for (idx in fields) {
                if (fields[idx].javadoc) {
                    --helpBlocks; %>
                "<%= fields[idx].fieldName %>": "<%= fields[idx].javadoc %>"<% if (helpBlocks > 0) { %>,<% }
                }
            } %>
            }<% } %>
        }
    }
<%_ if (microserviceAppName && !readOnly) { _%>,
    "<%= microserviceAppName %>": {
        "<%= entityTranslationKey %>" : {
            "created": "{{ param }} හඳුනාගැනීමෙන් නව <%= entityClassHumanized %> සාදනු ලැබේ",
            "updated": "{{ param }} හඳුනාගැනීමෙන් <%= entityClassHumanized %> යාවත්කාලීන වේ",
            "deleted": "{{ param }} හඳුනාගැනීමෙන් <%= entityClassHumanized %> මකා දමනු ලැබේ"
        }
    }
<%_ } _%>
}
