{
  "version": "0.0.0",
  "name": "<%= dasherizedBaseName %>",
  "appPath": "<%= MAIN_SRC_DIR %>",
  "testPath": "<%= TEST_SRC_DIR %>spec",
  "dependencies": {
    "angular": "1.5.5",
    "angular-aria": "1.5.5",
    "angular-bootstrap": "1.3.3",
    "angular-cache-buster": "0.4.3",
    "angular-cookies": "1.5.5",
    <%_ if (enableTranslation) { _%>
    "angular-dynamic-locale": "0.1.32",
    "angular-i18n": "1.5.5",
    <%_ } _%>
    "angular-loading-bar": "0.9.0",
    "angular-resource": "1.5.5",
    "angular-sanitize": "1.5.5",
    <%_ if (enableTranslation) { _%>
    "angular-translate": "2.11.0",
    "angular-translate-interpolation-messageformat": "2.11.0",
    "angular-translate-loader-partial": "2.11.0",
    "angular-translate-storage-cookie": "2.11.0",
    <%_ } _%>
    <%_ if (useSass) { _%>
    "bootstrap-sass": "bootstrap#v4.0.0-alpha.4",
    <%_ } else { _%>
    "bootstrap": "3.3.6",
    <%_ } _%>
    "json3": "3.3.2",
    "messageformat": "0.3.1",
    "modernizr": "3.3.1",
    "ng-file-upload": "12.0.4",
    "ngInfiniteScroll": "1.2.2",
    <%_ if (websocket == 'spring-websocket') { _%>
    "sockjs-client": "1.1.1",
    "stomp-websocket": "2.3.4",
    <%_ } _%>
    "swagger-ui": "2.1.4"
  },
  "devDependencies": {
    "angular-mocks": "1.5.5"
  },
  "overrides": {
    "angular-cache-buster" : {
      "dependencies": {
        "angular": "1.5.5"
      }
    },
    <%_ if (enableTranslation) { _%>
    "angular-dynamic-locale" : {
      "dependencies": {
        "angular": "1.5.5"
      }
    }
    <%_ } if (useSass) { _%>
    , "bootstrap-sass": {
      "main": [
        "scss/bootstrap.scss"
      ]
    }
    <%_ } _%>
  },
  "resolutions": {
    "angular": "1.5.5",
    "angular-cookies": "1.5.5",
    "angular-bootstrap": "1.3.3"
  }
}
