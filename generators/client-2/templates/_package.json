{
  "name": "<%= dasherizedBaseName %>",
  "version": "0.0.0",
  "description": "Description for <%= baseName %>",
  "private": true,
  "cacheDirectories": [
    "node_modules",
    "<%= MAIN_SRC_DIR %>bower_components"
  ],
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "@angular/upgrade":  "2.0.0-rc.6",

    "core-js": "2.4.1",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "zone.js": "0.6.17",
    "reflect-metadata": "0.1.3"
  },
  "devDependencies": {
    "bower": "1.7.9",
    "browser-sync": "2.15.0",
    "del": "2.2.2",
    "eslint-config-angular": "0.5.0",
    "eslint-plugin-angular": "1.3.1",
    "event-stream": "3.3.4",
    "generator-jhipster": "<%= packagejs.version %>",
    "gulp": "3.9.1",
    "gulp-angular-templatecache": "2.0.0",
    "gulp-autoprefixer": "3.1.1",
    "gulp-changed": "1.3.2",
    "gulp-cssnano": "2.1.2",
    "gulp-eslint": "3.0.1",
    <%_ if (useSass) { _%>
    "gulp-expect-file": "0.0.7",
    <%_ } _%>
    "gulp-flatten": "0.3.1",
    "gulp-footer": "1.0.5",
    "gulp-htmlmin": "2.0.0",
    "gulp-if": "2.0.1",
    "gulp-imagemin": "3.0.3",
    "gulp-inject": "4.1.0",
    "gulp-ng-annotate": "2.0.0",
    "gulp-ng-constant": "1.1.0",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    <%_ if (testFrameworks.indexOf('protractor') > -1) { _%>
    "gulp-protractor": "2.4.0",
    "gulp-util": "3.0.7",
    <%_ } _%>
    "gulp-rev": "7.1.2",
    "gulp-rev-replace": "0.4.3",
    <%_ if (useSass) { _%>
    "gulp-sass": "2.3.1",
    <%_ } _%>
    "gulp-sourcemaps": "1.6.0",
    "gulp-tslint": "2.0.0",
    "gulp-typescript": "2.13.6",
    "gulp-uglify": "2.0.0",
    "gulp-useref": "3.1.2",
    "jasmine-core": "2.5.0",
    <%_ if (testFrameworks.indexOf('protractor') > -1) { _%>
    "jasmine-reporters": "2.1.1",
    <%_ } _%>
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-junit-reporter": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-script-launcher": "1.0.0",
    "lazypipe": "1.0.1",
    "lodash": "4.15.0",
    "main-bower-files": "2.13.1",
    "map-stream": "0.0.6",
    "phantomjs-prebuilt": "2.1.12",
    <%_ if (testFrameworks.indexOf('protractor') > -1) { _%>
    "protractor": "3.3.0",
    "protractor-jasmine2-screenshot-reporter": "0.3.1",
    <%_ } _%>
    "proxy-middleware": "0.15.0",
    "run-sequence": "1.2.2",
    "typescript ": "1.8.10",
    "typings ": "1.3.2",
    <%_ if (buildTool == 'maven') { _%>
    "xml2js": "0.4.17",
    <%_ } _%>
    "yargs": "5.0.0"
  },
  "engines": {
    "node": "^4.3"
  },
  "scripts": {
    "test": "gulp test",
    "postinstall": "typings install<% if (testFrameworks.indexOf('protractor') > -1) { %> && webdriver-manager update<% } %>",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  }
}
