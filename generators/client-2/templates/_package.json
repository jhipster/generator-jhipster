    "bootstrap": "^4.0.0-alpha.4",
    "font-awesome": "4.7.0",
    "core-js": "2.4.1",
    "jquery": "3.1.0",
    <%_ if (enableTranslation){ _%>
    "ng2-translate": "3.1.1",
    <%_ } _%>
    "ng2-webstorage": "1.3.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
