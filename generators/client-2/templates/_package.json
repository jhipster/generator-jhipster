    "protractor": "3.3.0",
    "protractor-jasmine2-screenshot-reporter": "0.3.1",
    <%_ } _%>
    "proxy-middleware": "0.15.0",
    "run-sequence": "1.2.2",
    "typescript": "^2.0.3",
    <%_ if (buildTool == 'maven') { _%>
    "xml2js": "0.4.17",
    <%_ } _%>
    "yargs": "5.0.0"
  },
  "engines": {
    "node": "^4.3"
  },
  "scripts": {
    "test": "gulp test",
    <%_ if (testFrameworks.indexOf('protractor') > -1) { _%>
    "postinstall": "webdriver-manager update",
    <%_ } _%>
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  }
}
