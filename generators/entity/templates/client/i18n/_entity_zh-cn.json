{
    "<%= angularAppName %>": {
        "<%= entityTranslationKey %>" : {
            "home": {
                "title": "<%- formatAsApiDescription(javadoc) %>",
                "createLabel": "创建新<%- formatAsApiDescription(javadoc) %>",
                "createOrEditLabel": "创建或编辑<%- formatAsApiDescription(javadoc) %>"<% if (searchEngine === 'elasticsearch') { %>,
                "search": "查找<%- formatAsApiDescription(javadoc) %>"<% } %>
            },<% if (!microserviceAppName) { %>
            "created": "<%- formatAsApiDescription(javadoc) %> {{ param }} 创建成功",
            "updated": "<%- formatAsApiDescription(javadoc) %> {{ param }} 更新成功",
            "deleted": "<%- formatAsApiDescription(javadoc) %> {{ param }} 删除成功",<% } %>
            "delete": {
                "question": "你确定要删除<%- formatAsApiDescription(javadoc) %> {{ id }} 吗？"
            },
            "detail": {
                "title": "<%- formatAsApiDescription(javadoc) %>"
            }<% for (idx in fields) { %>,
            "<%=fields[idx].fieldName%>": "<%= formatAsApiDescription(fields[idx].javadoc) %>"<% } %><% for (idx in relationships) { %>,
            "<%=relationships[idx].relationshipName%>": "<%= formatAsApiDescription(relationships[idx].javadoc) %>"<% } %>
        }
    }<% if (microserviceAppName) { %>,
    "<%= microserviceAppName %>": {
        "<%= entityTranslationKey %>" : {
            "created": "<%- formatAsApiDescription(javadoc) %> {{ param }} 创建成功",
            "updated": "<%- formatAsApiDescription(javadoc) %> {{ param }} 更新成功",
            "deleted": "<%- formatAsApiDescription(javadoc) %> {{ param }} 删除成功"
        }
    }<% } %>
}
