<%#	Returns a string of all validator annotations for the entity relationships. -%>
<%
var relationship = relationships[idx];
var result = '';
if (relationship.relationshipValidate === true) {
    var validators = [];
    if (relationship.relationshipRequired) {
        validators.push('@NotNull');
    }
    result =  validators.join('\n    ');
    result += '\n';
} -%>
<%- result -%>
