<%#	Returns a string of all validator annotations for the entity realtionships. -%>
<%
var relationship = relationships[idx];
var result = '';

if (relationship.relationshipValidate == true) {
    var rules = relationship.relationshipValidateRules;
    var validators = [];

    if (rules.indexOf('required') != -1) {
        validators.push('@NotNull');
    }
    result =  validators.join('\n    ');
    result += '\n';
} -%>
<%- result -%>
