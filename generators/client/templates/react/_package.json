    "react-addons-transition-group": "15.6.0",
    "react-dom": "15.6.1",
    "react-interpolate-component": "0.11.0",
    "react-redux": "5.0.5",
    "react-redux-loading-bar": "2.9.2",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "react-transition-group": "1.2.0",
    "react-translate-component": "0.14.0",
