let StorageMock = () => {
  let store = {};
  return {
    get: (key: any) => store[key] || null,
    set: (key: any, value: any) => (store[key] = value.toString()),
    clear: () => (store = {})
  };
};

Object.defineProperty(window, 'localStorage', {
  value: StorageMock()
});

Object.defineProperty(window, 'sessionStorage', {
  value: StorageMock()
});
