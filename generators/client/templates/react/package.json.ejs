{
  "name": "<%= dasherizedBaseName %>",
  "version": "0.0.1-SNAPSHOT",
  "description": "Description for <%= baseName %>",
  "private": true,
  "license": "UNLICENSED",
  "cacheDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "<%= packageJson.dependencies['@fortawesome/fontawesome-svg-core'] %>",
    "@fortawesome/free-solid-svg-icons": "<%= packageJson.dependencies['@fortawesome/free-solid-svg-icons'] %>",
    "@fortawesome/react-fontawesome": "<%= packageJson.dependencies['@fortawesome/react-fontawesome'] %>",
    "availity-reactstrap-validation": "<%= packageJson.dependencies['availity-reactstrap-validation'] %>",
    "axios": "<%= packageJson.dependencies['axios'] %>",
    "bootstrap": "<%= packageJson.dependencies['bootstrap'] %>",
    <%_ if (clientTheme !== 'none') { _%>
    "bootswatch": "<%= packageJson.dependencies['bootswatch'] %>",
    <%_ } _%>
    "dayjs": "<%= packageJson.dependencies['dayjs'] %>",
    "loaders.css": "<%= packageJson.dependencies['loaders.css'] %>",
    "lodash": "<%= packageJson.dependencies['lodash'] %>",
    "path-browserify": "<%= packageJson.dependencies['path-browserify'] %>",
    "react": "<%= packageJson.dependencies['react'] %>",
    "react-dom": "<%= packageJson.dependencies['react-dom'] %>",
    "react-hot-loader": "<%= packageJson.dependencies['react-hot-loader'] %>",
    "react-jhipster": "<%= packageJson.dependencies['react-jhipster'] %>",
    "react-loadable": "<%= packageJson.dependencies['react-loadable'] %>",
    "react-redux": "<%= packageJson.dependencies['react-redux'] %>",
    "react-redux-loading-bar": "<%= packageJson.dependencies['react-redux-loading-bar'] %>",
    "react-router-dom": "<%= packageJson.dependencies['react-router-dom'] %>",
    "react-toastify": "<%= packageJson.dependencies['react-toastify'] %>",
    "react-transition-group": "<%= packageJson.dependencies['react-transition-group'] %>",
    "reactstrap": "<%= packageJson.dependencies['reactstrap'] %>",
    "redux": "<%= packageJson.dependencies['redux'] %>",
    "redux-devtools": "<%= packageJson.dependencies['redux-devtools'] %>",
    "redux-devtools-dock-monitor": "<%= packageJson.dependencies['redux-devtools-dock-monitor'] %>",
    "redux-devtools-log-monitor": "<%= packageJson.dependencies['redux-devtools-log-monitor'] %>",
    "redux-promise-middleware": "<%= packageJson.dependencies['redux-promise-middleware'] %>",
    "redux-thunk": "<%= packageJson.dependencies['redux-thunk'] %>",
    <%_ if (websocket === 'spring-websocket') { _%>
    "rxjs": "6.6.3",
    "sockjs-client": "1.5.0",
    <%_ } _%>
    <%_ if (skipServer) { _%>
    "sonar-scanner": "<%= packageJson.dependencies['sonar-scanner'] %>",
    <%_ } _%>
    "tslib": "<%= packageJson.dependencies['tslib'] %>",
    "uuid": "<%= packageJson.dependencies['uuid'] %>"<% if (websocket === 'spring-websocket') { %>,
    "webstomp-client": "1.2.6"
    <%_ } _%>
  },
  "devDependencies": {
    "@testing-library/react": "<%= packageJson.devDependencies['@testing-library/react'] %>",
    <%_ if (protractorTests) { _%>
    "@types/chai": "4.2.11",
    <%_ } _%>
    "@types/jest": "<%= packageJson.devDependencies['@types/jest'] %>",
    "@types/lodash": "<%= packageJson.devDependencies['@types/lodash'] %>",
    <%_ if (protractorTests) { _%>
    "@types/mocha": "7.0.2",
    <%_ } _%>
    "@types/node": "<%= packageJson.devDependencies['@types/node'] %>",
    "@types/react": "<%= packageJson.devDependencies['@types/react'] %>",
    "@types/react-dom": "<%= packageJson.devDependencies['@types/react-dom'] %>",
    "@types/react-redux": "<%= packageJson.devDependencies['@types/react-redux'] %>",
    "@types/react-router-dom": "<%= packageJson.devDependencies['@types/react-router-dom'] %>",
    "@types/redux": "<%= packageJson.devDependencies['@types/redux'] %>",
    <%_ if (protractorTests) { _%>
    "@types/selenium-webdriver": "4.0.9",
    <%_ } _%>
    "@types/webpack-env": "<%= packageJson.devDependencies['@types/webpack-env'] %>",
    "@typescript-eslint/eslint-plugin": "<%= packageJson.devDependencies['@typescript-eslint/eslint-plugin'] %>",
    "@typescript-eslint/parser": "<%= packageJson.devDependencies['@typescript-eslint/parser'] %>",
    "autoprefixer": "<%= packageJson.devDependencies['autoprefixer'] %>",
    "browser-sync": "<%= packageJson.devDependencies['browser-sync'] %>",
    "browser-sync-webpack-plugin": "<%= packageJson.devDependencies['browser-sync-webpack-plugin'] %>",
    "cache-loader": "<%= packageJson.devDependencies['cache-loader'] %>",
    <%_ if (protractorTests) { _%>
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    <%_ } _%>
    "copy-webpack-plugin": "<%= packageJson.devDependencies['copy-webpack-plugin'] %>",
    "core-js": "<%= packageJson.devDependencies['core-js'] %>",
    "cross-env": "<%= packageJson.devDependencies['cross-env'] %>",
    "css-loader": "<%= packageJson.devDependencies['css-loader'] %>",
    "eslint": "<%= packageJson.devDependencies['eslint'] %>",
    "eslint-config-prettier": "<%= packageJson.devDependencies['eslint-config-prettier'] %>",
    "eslint-plugin-react": "<%= packageJson.devDependencies['eslint-plugin-react'] %>",
    "eslint-webpack-plugin": "<%= packageJson.devDependencies['eslint-webpack-plugin'] %>",
    "file-loader": "<%= packageJson.devDependencies['file-loader'] %>",
    "fork-ts-checker-webpack-plugin": "<%= packageJson.devDependencies['fork-ts-checker-webpack-plugin'] %>",
    "friendly-errors-webpack-plugin": "<%= packageJson.devDependencies['friendly-errors-webpack-plugin'] %>",
    <%_ if (!skipJhipsterDependencies) { _%>
    "generator-jhipster": "<%= packagejs.version %>",
        <%_ otherModules.forEach(module => { _%>
    "<%= module.name %>": "<%= module.version %>",
        <%_ });
    } _%>
    "html-webpack-plugin": "<%= packageJson.devDependencies['html-webpack-plugin'] %>",
    "identity-obj-proxy": "<%= packageJson.devDependencies['identity-obj-proxy'] %>",
    "jest": "<%= packageJson.devDependencies['jest'] %>",
    "jest-junit": "<%= packageJson.devDependencies['jest-junit'] %>",
    "jest-sonar-reporter": "<%= packageJson.devDependencies['jest-sonar-reporter'] %>",
    "json-loader": "<%= packageJson.devDependencies['json-loader'] %>",
    <%_ if (!skipCommitHook) { _%>
    "husky": "<%= packageJson.devDependencies['husky'] %>",
    "lint-staged": "<%= packageJson.devDependencies['lint-staged'] %>",
    <%_ } _%>
    <%_ if (enableTranslation) { _%>
    "merge-jsons-webpack-plugin": "<%= packageJson.devDependencies['merge-jsons-webpack-plugin'] %>",
    <%_ } _%>
    "mini-css-extract-plugin": "<%= packageJson.devDependencies['mini-css-extract-plugin'] %>",
    <%_ if (protractorTests) { _%>
    "mocha": "7.1.2",
    <%_ } _%>
    "optimize-css-assets-webpack-plugin": "<%= packageJson.devDependencies['optimize-css-assets-webpack-plugin'] %>",
    "postcss-loader": "<%= packageJson.devDependencies['postcss-loader'] %>",
    "prettier": "<%= packageJson.devDependencies['prettier'] %>",
    <%_ if (!skipServer) { _%>
    "prettier-plugin-java": "<%= packageJson.devDependencies['prettier-plugin-java'] %>",
    <%_ } _%>
    <%_ if (protractorTests) { _%>
    "protractor": "7.0.0",
    <%_ } _%>
    "react-infinite-scroller": "<%= packageJson.devDependencies['react-infinite-scroller'] %>",
    "redux-mock-store": "<%= packageJson.devDependencies['redux-mock-store'] %>",
    "rimraf": "<%= packageJson.devDependencies['rimraf'] %>",
    "sass": "<%= packageJson.devDependencies['sass'] %>",
    "sass-loader": "<%= packageJson.devDependencies['sass-loader'] %>",
    "simple-progress-webpack-plugin": "<%= packageJson.devDependencies['simple-progress-webpack-plugin'] %>",
    "sinon": "<%= packageJson.devDependencies['sinon'] %>",
    "source-map-loader": "<%= packageJson.devDependencies['source-map-loader'] %>",
    "sourcemap-istanbul-instrumenter-loader": "<%= packageJson.devDependencies['sourcemap-istanbul-instrumenter-loader'] %>",
    "stripcomment-loader": "<%= packageJson.devDependencies['stripcomment-loader'] %>",
    "style-loader": "<%= packageJson.devDependencies['style-loader'] %>",
    "swagger-ui-dist": "<%= packageJson.devDependencies['swagger-ui-dist'] %>",
    "terser-webpack-plugin": "<%= packageJson.devDependencies['terser-webpack-plugin'] %>",
    "thread-loader": "<%= packageJson.devDependencies['thread-loader'] %>",
    "to-string-loader": "<%= packageJson.devDependencies['to-string-loader'] %>",
    "ts-jest": "<%= packageJson.devDependencies['ts-jest'] %>",
    "ts-loader": "<%= packageJson.devDependencies['ts-loader'] %>",
    <%_ if (protractorTests) { _%>
    "ts-node": "8.9.1",
    <%_ } _%>
    <%_ if (cypressTests) { _%>
    "cypress": "<%= packageJson.devDependencies['cypress'] %>",
    <%_ if (authenticationType === 'oauth2') { _%>
    "puppeteer": "<%= packageJson.devDependencies['puppeteer'] %>",
    <%_ } _%>
    <%_ } _%>
    "typescript": "<%= packageJson.devDependencies['typescript'] %>",
    <%_ if (protractorTests) { _%>
    "webdriver-manager": "12.1.8",
    <%_ } _%>
    "webpack": "<%= packageJson.devDependencies['webpack'] %>",
    "webpack-cli": "<%= packageJson.devDependencies['webpack-cli'] %>",
    "webpack-dev-server": "<%= packageJson.devDependencies['webpack-dev-server'] %>",
    "webpack-merge": "<%= packageJson.devDependencies['webpack-merge'] %>",
    "webpack-notifier": "<%= packageJson.devDependencies['webpack-notifier'] %>",
    "workbox-webpack-plugin": "<%= packageJson.devDependencies['workbox-webpack-plugin'] %>"
  },
  "engines": {
    "node": ">=<%= NODE_VERSION %>"
  },
  "config": {
    "default_environment": "prod"
  },
  "scripts": {
    "prettier:check": "prettier --check \"{,src/**/,webpack/}*.{<%= getPrettierExtensions() %>}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{<%= getPrettierExtensions() %>}\"",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "<%= clientPackageManager %> run lint -- --fix",
    "cleanup": "rimraf <%= DIST_DIR %>",
    "clean-www": "rimraf <%= DIST_DIR %>app/{src,<%= BUILD_DIR %>}",
    <%_ if (protractorTests && !cypressTests) { _%>
    "e2e": "<%= clientPackageManager %> run e2e:protractor",
    "e2e:headless": "<%= clientPackageManager %> run e2e:protractor:headless",
    <%_ } else if ((cypressTests && !protractorTests) || (protractorTests && cypressTests)) {  _%>
    "e2e": "<%= clientPackageManager %> run e2e:cypress",
    "e2e:headless": "<%= clientPackageManager %> run e2e:cypress:headless",
    <%_ } _%>
    <%_ if (protractorTests) { _%>
    "e2e:protractor:headless": "<%= clientPackageManager %> run e2e:protractor",
    "e2e:protractor": "protractor <%= TEST_SRC_DIR %>protractor.conf.js",
    <%_ } _%>
    <%_ if (cypressTests) { _%>
    "e2e:cypress:headless": "<%= clientPackageManager %> run e2e:cypress -- --headless",
    "e2e:cypress": "cypress run --browser chrome",
    "cypress": "cypress open",
    <%_ } _%>
    <%_ if (protractorTests) { _%>
    "postinstall": "<%= clientPackageManager %> run e2e:update-webdriver",
    "e2e:update-webdriver": "webdriver-manager update --gecko false",
    <%_ } _%>
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config jest.conf.js",
    "jest:update": "<%= clientPackageManager %> run jest -- --updateSnapshot",
    "start": "<%= clientPackageManager %> run webapp:dev",
    "start-tls": "<%= clientPackageManager %> run webapp:dev -- --env.tls",
    <%_ if (skipServer) { _%>
    "sonar": "sonar-scanner",
    <%_ } _%>
    "pretest": "<%= clientPackageManager %> run lint",
    "test": "<%= clientPackageManager %> run jest",
    "test-ci": "<%= clientPackageManager %> run lint && <%= clientPackageManager %> run jest:update",
    "test:watch": "<%= clientPackageManager %> run jest -- --watch",
    "webapp:build": "<%= clientPackageManager %> run clean-www && <%= clientPackageManager %> run webapp:build:dev",
    "webapp:build:dev": "<%= clientPackageManager %> run webpack -- --config webpack/webpack.dev.js --env stats=minimal",
    "webapp:build:prod": "<%= clientPackageManager %> run webpack -- --config webpack/webpack.prod.js --progress=profile",
    "webapp:dev": "<%= clientPackageManager %> run webpack-dev-server -- --config webpack/webpack.dev.js --inline --port=9060 --env stats=minimal",
    "webapp:dev-verbose": "<%= clientPackageManager %> run webpack-dev-server -- --config webpack/webpack.dev.js --inline --port=9060 --progress=profile --env stats=normal",
    "webapp:prod": "<%= clientPackageManager %> run clean-www && <%= clientPackageManager %> run webapp:build:prod",
    "webapp:test": "<%= clientPackageManager %> run test",
    "webpack-dev-server": "npm run webpack -- serve",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "jestSonar": {
    "reportPath": "<%= BUILD_DIR %>test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
