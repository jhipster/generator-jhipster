{
  "name": "<%= dasherizedBaseName %>",
  "version": "0.0.0",
  "description": "Description for <%= baseName  %>",
  "private": true,
  "license": "UNLICENSED",
  "cacheDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@fortawesome/free-solid-svg-icons": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@fortawesome/vue-fontawesome": "VERSION_MANAGED_BY_CLIENT_VUE",
    "axios": "VERSION_MANAGED_BY_CLIENT_VUE",
    "bootstrap": "VERSION_MANAGED_BY_CLIENT_VUE",
    "bootstrap-vue": "VERSION_MANAGED_BY_CLIENT_VUE",
    <%_ if (clientTheme !== 'none') { _%>
    "bootswatch": "VERSION_MANAGED_BY_CLIENT_VUE",
    <%_ } _%>
    "dayjs": "VERSION_MANAGED_BY_CLIENT_COMMON",
    "swagger-ui-dist": "VERSION_MANAGED_BY_CLIENT_VUE",
    <%_ if (websocket === 'spring-websocket') { _%>
    "sockjs-client": "1.1.4",
    "webstomp-client": "1.2.0",
    <%_ } _%>
    "vue": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vue-class-component": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vue-cookie": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vue-i18n": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vue-infinite-loading": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vue-property-decorator": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vue-router": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vue2-filters": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vuelidate": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vuex": "VERSION_MANAGED_BY_CLIENT_VUE"
  },
  "devDependencies": {
    <%_ if (protractorTests) { _%>
    "@types/chai": "4.2.11",
    "@types/chai-string": "1.4.2",
    "@types/mocha": "7.0.2",
    "@types/selenium-webdriver": "4.0.9",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "1.5.0",
    "mocha": "7.1.2",
    "protractor": "5.4.4",
    "ts-node": "8.10.1",
    "webdriver-manager": "12.1.8",
    <%_ } _%>
    "babel-core": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@types/jest": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@types/node": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@types/sinon": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@types/vuelidate": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@typescript-eslint/eslint-plugin": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@typescript-eslint/parser": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@vue/cli-plugin-eslint": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@vue/cli-plugin-typescript": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@vue/cli-service": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@vue/eslint-config-prettier": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@vue/eslint-config-typescript": "VERSION_MANAGED_BY_CLIENT_VUE",
    "@vue/test-utils": "VERSION_MANAGED_BY_CLIENT_VUE",
    "autoprefixer": "VERSION_MANAGED_BY_CLIENT_VUE",
    "browser-sync": "VERSION_MANAGED_BY_CLIENT_VUE",
    "browser-sync-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_VUE",
    "copy-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_VUE",
    "css-loader": "VERSION_MANAGED_BY_CLIENT_VUE",
    "file-loader": "VERSION_MANAGED_BY_CLIENT_VUE",
    "fork-ts-checker-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_VUE",
    "friendly-errors-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_VUE",
<%_ if (!skipJhipsterDependencies) { _%>
    "generator-jhipster": "<%= packagejs.version %>",
    <%_ otherModules.forEach(module => { _%>
    "<%= module.name %>": "<%= module.version %>",
    <%_ });
} _%>
    "html-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_VUE",
    <%_ if (!skipCommitHook) { _%>
    "husky": "VERSION_MANAGED_BY_CLIENT_COMMON",
    "lint-staged": "VERSION_MANAGED_BY_CLIENT_COMMON",
    <%_ } _%>
    "jest": "VERSION_MANAGED_BY_CLIENT_VUE",
    "jest-junit": "VERSION_MANAGED_BY_CLIENT_VUE",
    "jest-serializer-vue": "VERSION_MANAGED_BY_CLIENT_VUE",
    "jest-sonar-reporter": "VERSION_MANAGED_BY_CLIENT_VUE",
    "jest-vue-preprocessor": "VERSION_MANAGED_BY_CLIENT_VUE",
    "merge-jsons-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_VUE",
    "mini-css-extract-plugin": "VERSION_MANAGED_BY_CLIENT_VUE",
    "node-notifier": "VERSION_MANAGED_BY_CLIENT_VUE",
    "numeral": "VERSION_MANAGED_BY_CLIENT_VUE",
    "optimize-css-assets-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_VUE",
    "portfinder": "VERSION_MANAGED_BY_CLIENT_VUE",
    "postcss": "VERSION_MANAGED_BY_CLIENT_VUE",
    "postcss-import": "VERSION_MANAGED_BY_CLIENT_VUE",
    "postcss-loader": "VERSION_MANAGED_BY_CLIENT_VUE",
    "postcss-url": "VERSION_MANAGED_BY_CLIENT_VUE",
    "prettier": "<%= PRETTIER_VERSION %>",
    <%_ if (!skipServer) { _%>
    "prettier-plugin-java": "<%= PRETTIER_JAVA_VERSION %>",
    <%_ } _%>
    <%_ if (cypressTests) { _%>
    "cypress": "VERSION_MANAGED_BY_CLIENT_COMMON",
    <%_ if (authenticationType === 'oauth2') { _%>
    "puppeteer": "VERSION_MANAGED_BY_CLIENT_COMMON",
    <%_ } _%>
    <%_ } _%>
    "rimraf": "VERSION_MANAGED_BY_CLIENT_VUE",
    "sass": "VERSION_MANAGED_BY_CLIENT_VUE",
    "sass-loader": "VERSION_MANAGED_BY_CLIENT_VUE",
    "sinon": "VERSION_MANAGED_BY_CLIENT_VUE",
    "terser-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_VUE",
    "ts-jest": "VERSION_MANAGED_BY_CLIENT_VUE",
    "ts-loader": "VERSION_MANAGED_BY_CLIENT_VUE",
    "tslib": "VERSION_MANAGED_BY_CLIENT_VUE",
    "eslint": "VERSION_MANAGED_BY_CLIENT_VUE",
    "tslint-config-prettier": "VERSION_MANAGED_BY_CLIENT_VUE",
    "eslint-plugin-prettier": "VERSION_MANAGED_BY_CLIENT_VUE",
    "tslint-eslint-rules": "VERSION_MANAGED_BY_CLIENT_VUE",
    "eslint-plugin-vue": "VERSION_MANAGED_BY_CLIENT_VUE",
    "tslint-loader": "VERSION_MANAGED_BY_CLIENT_VUE",
    "typescript": "VERSION_MANAGED_BY_CLIENT_VUE",
    "url-loader": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vue-jest": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vue-loader": "VERSION_MANAGED_BY_CLIENT_VUE",
    "vue-template-compiler": "VERSION_MANAGED_BY_CLIENT_VUE",
    "webpack": "VERSION_MANAGED_BY_CLIENT_VUE",
    "webpack-bundle-analyzer": "VERSION_MANAGED_BY_CLIENT_VUE",
    "webpack-cli": "VERSION_MANAGED_BY_CLIENT_VUE",
    "webpack-dev-server": "VERSION_MANAGED_BY_CLIENT_VUE",
    "webpack-merge": "VERSION_MANAGED_BY_CLIENT_VUE",
    "workbox-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_VUE"
  },
  "engines": {
    "node": ">=<%= NODE_VERSION %>",
    "npm": ">= 6.14.4"
  },
  "config": {
    "default_environment": "prod"
  },
  "scripts": {
    "prettier:check": "prettier --check \"{,src/**/,webpack/}*.{<%= getPrettierExtensions() %>}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{<%= getPrettierExtensions() %>}\"",
    "lint": "vue-cli-service lint --no-fix",
    "lint:fix": "vue-cli-service lint",
    "cleanup": "rimraf <%= DIST_DIR %>",
    "clean-www": "rimraf <%= DIST_DIR %>app/{src,<%= BUILD_DIR %>}",
    "start": "<%= clientPackageManager %> run webapp:dev",
    "start-tls": "<%= clientPackageManager %> run webapp:dev -- --env.tls",
    "serve": "<%= clientPackageManager %> run start",
    "build": "<%= clientPackageManager %> run webapp:prod",
    <%_ if (protractorTests && !cypressTests) { _%>
    "e2e": "<%= clientPackageManager %> run e2e:protractor",
    "e2e:headless": "<%= clientPackageManager %> run e2e:protractor:headless",
    <%_ } else if ((cypressTests && !protractorTests) || (protractorTests && cypressTests)) {  _%>
    "e2e": "<%= clientPackageManager %> run e2e:cypress",
    "e2e:headless": "<%= clientPackageManager %> run e2e:cypress:headless",
    <%_ } _%>
    <%_ if (protractorTests) { _%>
    "e2e:protractor:headless": "<%= clientPackageManager %> run e2e:protractor",
    "e2e:protractor": "protractor <%= TEST_SRC_DIR %>protractor.conf.js",
    <%_ } _%>
    <%_ if (cypressTests) { _%>
    "e2e:cypress:headless": "<%= clientPackageManager %> run e2e:cypress -- --headless",
    "e2e:cypress": "cypress run --browser chrome",
    "cypress": "cypress open",
    <%_ } _%>
    <%_ if (protractorTests) { _%>
    "postinstall": "<%= clientPackageManager %> run e2e:update-webdriver",
    "e2e:update-webdriver": "webdriver-manager update --gecko false",
    <%_ } _%>
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --no-cache --config <%= CLIENT_TEST_SRC_DIR %>jest.conf.js",
    "jest:update": "<%= clientPackageManager %> run jest -- --updateSnapshot",
    "pretest": "<%= clientPackageManager %> run lint",
    "test": "<%= clientPackageManager %> run jest",
    "test-ci": "<%= clientPackageManager %> run lint && <%= clientPackageManager %> run jest:update",
    "test:watch": "<%= clientPackageManager %> run jest -- --watch",
    "webapp:build": "<%= clientPackageManager %> run cleanup && <%= clientPackageManager %> run webapp:build:dev",    
    "webapp:build:dev": "<%= clientPackageManager %> run webpack -- --config webpack/webpack.dev.js --progress=profile --env stats=minimal",
    "webapp:build:prod": "<%= clientPackageManager %> run webpack -- --config webpack/webpack.prod.js --progress=profile --env stats=minimal",
    "webapp:dev": "<%= clientPackageManager %> run webpack-dev-server -- --config webpack/webpack.dev.js --progress=profile --inline --env stats=normal",
    "webapp:prod": "<%= clientPackageManager %> run cleanup && <%= clientPackageManager %> run webapp:build:prod && <%= clientPackageManager %> run clean-www",
    "webapp:test": "<%= clientPackageManager %> run test",
    "webpack-dev-server": "webpack serve",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "jestSonar": {
    "reportPath": "<%= BUILD_DIR %>test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
