{
  "name": "<%= dasherizedBaseName %>",
  "version": "0.0.1-SNAPSHOT",
  "description": "Description for <%= baseName %>",
  "private": true,
  "license": "UNLICENSED",
  "cacheDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "@angular/common": "<%= packageJson.dependencies['@angular/common'] %>",
    "@angular/compiler": "<%= packageJson.dependencies['@angular/common'] %>",
    "@angular/core": "<%= packageJson.dependencies['@angular/common'] %>",
    "@angular/forms": "<%= packageJson.dependencies['@angular/common'] %>",
    "@angular/localize": "<%= packageJson.dependencies['@angular/common'] %>",
    "@angular/platform-browser": "<%= packageJson.dependencies['@angular/common'] %>",
    "@angular/platform-browser-dynamic": "<%= packageJson.dependencies['@angular/common'] %>",
    "@angular/router": "<%= packageJson.dependencies['@angular/common'] %>",
    "@fortawesome/angular-fontawesome": "<%= packageJson.dependencies['@fortawesome/angular-fontawesome'] %>",
    "@fortawesome/fontawesome-svg-core": "<%= packageJson.dependencies['@fortawesome/fontawesome-svg-core'] %>",
    "@fortawesome/free-solid-svg-icons": "<%= packageJson.dependencies['@fortawesome/free-solid-svg-icons'] %>",
    "@ng-bootstrap/ng-bootstrap": "<%= packageJson.dependencies['@ng-bootstrap/ng-bootstrap'] %>",
<%_ if (enableTranslation) { _%>
    "@ngx-translate/core": "<%= packageJson.dependencies['@ngx-translate/core'] %>",
    "@ngx-translate/http-loader": "<%= packageJson.dependencies['@ngx-translate/http-loader'] %>",
<%_ } _%>
    "bootstrap": "<%= packageJson.dependencies.bootstrap %>",
    <%_ if (clientTheme !== 'none') { _%>
    "bootswatch": "<%= packageJson.dependencies.bootstrap %>",
    <%_ } _%>
    "dayjs": "<%= packageJson.dependencies['dayjs'] %>",
    <%_ if (authenticationType === 'session' && websocket === 'spring-websocket') { _%>
    "ngx-cookie-service": "<%= packageJson.dependencies['ngx-cookie-service'] %>",
    <%_ } _%>
    "ngx-infinite-scroll": "<%= packageJson.dependencies['ngx-infinite-scroll'] %>",
    "ngx-webstorage": "<%= packageJson.dependencies['ngx-webstorage'] %>",
    "rxjs": "<%= packageJson.dependencies['rxjs'] %>",
    "swagger-ui-dist": "<%= packageJson.dependencies['swagger-ui-dist'] %>",
    <%_ if (websocket === 'spring-websocket') { _%>
    "sockjs-client": "1.5.0",
    "webstomp-client": "1.2.6",
    <%_ } _%>
    <%_ if (skipServer) { _%>
    "sonar-scanner": "3.1.0",
    <%_ } _%>
    "tslib": "<%= packageJson.dependencies['tslib'] %>",
    "zone.js": "<%= packageJson.dependencies['zone.js'] %>"
  },
  "devDependencies": {
<%_ if (protractorTests) { _%>
    "@types/chai": "4.2.14",
    "@types/chai-string": "1.4.2",
    "@types/mocha": "8.2.0",
    "@types/selenium-webdriver": "4.0.11",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "1.5.0",
    "mocha": "8.2.1",
    "protractor": "7.0.0",
    "ts-node": "9.1.1",
    "webdriver-manager": "12.1.8",
<%_ } _%>
<%_ if (websocket === 'spring-websocket') { _%>
    "@types/sockjs-client": "1.5.0",
<%_ } _%>
<%_ if (!skipCommitHook) { _%>
    "husky": "<%= packageJson.devDependencies['husky'] %>",
    "lint-staged": "<%= packageJson.devDependencies['lint-staged'] %>",
<%_ } _%>
<%_ if (cypressTests) { _%>
    "cypress": "<%= packageJson.devDependencies['cypress'] %>",
<%_ if (authenticationType === 'oauth2') { _%>
    "puppeteer": "<%= packageJson.devDependencies['puppeteer'] %>",
<%_ } _%>
<%_ } _%>
<%_ if (enableTranslation) { _%>
    "merge-jsons-webpack-plugin": "<%= packageJson.devDependencies['merge-jsons-webpack-plugin'] %>",
<%_ } _%>
<%_ if (!skipServer) { _%>
    "prettier-plugin-java": "<%= packageJson.devDependencies['prettier-plugin-java'] %>",
<%_ } _%>
    "@angular/cli": "<%= packageJson.devDependencies['@angular/cli'] %>",
    "@angular/compiler-cli": "<%= packageJson.dependencies['@angular/common'] %>",
    "@angular-builders/custom-webpack": "<%= packageJson.devDependencies['@angular-builders/custom-webpack'] %>",
    "@angular-builders/jest": "<%= packageJson.devDependencies['@angular-builders/custom-webpack'] %>",
    "@angular/service-worker": "<%= packageJson.dependencies['@angular/common'] %>",
    "@angular-eslint/eslint-plugin": "<%= packageJson.devDependencies['@angular-eslint/eslint-plugin'] %>",
    "@types/jest": "<%= packageJson.devDependencies['@types/jest'] %>",
    "@types/node": "<%= packageJson.devDependencies['@types/node'] %>",
    "@typescript-eslint/eslint-plugin": "<%= packageJson.devDependencies['@typescript-eslint/eslint-plugin'] %>",
    "@typescript-eslint/parser": "<%= packageJson.devDependencies['@typescript-eslint/eslint-plugin'] %>",
    "browser-sync": "<%= packageJson.devDependencies['browser-sync'] %>",
    "browser-sync-webpack-plugin": "<%= packageJson.devDependencies['browser-sync-webpack-plugin'] %>",
    "copy-webpack-plugin": "<%= packageJson.devDependencies['copy-webpack-plugin'] %>",
    "eslint": "<%= packageJson.devDependencies['eslint'] %>",
    "eslint-config-prettier": "<%= packageJson.devDependencies['eslint-config-prettier'] %>",
    "eslint-webpack-plugin": "<%= packageJson.devDependencies['eslint-webpack-plugin'] %>",
    "friendly-errors-webpack-plugin": "<%= packageJson.devDependencies['friendly-errors-webpack-plugin'] %>",
<%_ if (!skipJhipsterDependencies) { _%>
    "generator-jhipster": "<%= packagejs.version %>",
    <%_ otherModules.forEach(module => { _%>
    "<%= module.name %>": "<%= module.version %>",
    <%_ });
} _%>
    "jest": "<%= packageJson.devDependencies['jest'] %>",
    "jest-date-mock": "<%= packageJson.devDependencies['jest-date-mock'] %>",
    "jest-junit": "<%= packageJson.devDependencies['jest-junit'] %>",
    "jest-sonar-reporter": "<%= packageJson.devDependencies['jest-sonar-reporter'] %>",
    "prettier": "<%= packageJson.devDependencies['prettier'] %>",
    "rimraf": "<%= packageJson.devDependencies['rimraf'] %>",
    "simple-progress-webpack-plugin": "<%= packageJson.devDependencies['simple-progress-webpack-plugin'] %>",
    "typescript": "<%= packageJson.devDependencies['typescript'] %>",
    "webpack": "<%= packageJson.devDependencies['webpack'] %>",
    "webpack-bundle-analyzer": "<%= packageJson.devDependencies['webpack-bundle-analyzer'] %>",
    "webpack-notifier": "<%= packageJson.devDependencies['webpack-notifier'] %>"
  },
  "engines": {
    "node": ">=<%= NODE_VERSION %>"
  },
  "config": {
    "default_environment": "prod"
  },
  "scripts": {
    "prettier:check": "prettier --check \"{,src/**/,webpack/}*.{<%= getPrettierExtensions() %>}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{<%= getPrettierExtensions() %>}\"",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "<%= clientPackageManager %> run lint -- --fix",
    "cleanup": "rimraf <%= DIST_DIR %>",
    "clean-www": "rimraf <%= DIST_DIR %>app/{src,<%= BUILD_DIR %>}",
    <%_ if (protractorTests && !cypressTests) { _%>
    "e2e": "<%= clientPackageManager %> run e2e:protractor",
    "e2e:headless": "<%= clientPackageManager %> run e2e:protractor:headless",
    <%_ } else if ((cypressTests && !protractorTests) || (protractorTests && cypressTests)) {  _%>
    "e2e": "<%= clientPackageManager %> run e2e:cypress",
    "e2e:headless": "<%= clientPackageManager %> run e2e:cypress:headless",
    <%_ } _%>
    <%_ if (protractorTests) { _%>
    "e2e:protractor:headless": "<%= clientPackageManager %> run e2e:protractor",
    "e2e:protractor": "protractor <%= TEST_SRC_DIR %>protractor.conf.js",
    <%_ } _%>
    <%_ if (cypressTests) { _%>
    "e2e:cypress:headless": "<%= clientPackageManager %> run e2e:cypress -- --headless",
    "e2e:cypress": "cypress run --browser chrome",
    "cypress": "cypress open",
    <%_ } _%>
    <%_ if (protractorTests) { _%>
    "postinstall": "<%= clientPackageManager %> run e2e:update-webdriver",
    "e2e:update-webdriver": "webdriver-manager update --gecko false",
    <%_ } _%>
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config jest.conf.js",
    "start": "<%= clientPackageManager %> run webapp:dev",
    "start-hmr": "ng serve --hmr",
    "start-tls": "<%= clientPackageManager %> run webapp:dev -- --env.tls",
    <%_ if (skipServer) { _%>
    "sonar": "sonar-scanner",
    <%_ } _%>
    "serve": "<%= clientPackageManager %> run start",
    "build": "<%= clientPackageManager %> run webapp:prod",
    "pretest": "<%= clientPackageManager %> run lint",
    "test": "ng test --coverage --log-heap-usage -w=2",
    "test:watch": "<%= clientPackageManager %> run test -- --watch",
    "webapp:build": "<%= clientPackageManager %> run clean-www && <%= clientPackageManager %> run webapp:build:dev",
    "webapp:build:dev": "ng build",
    "webapp:build:prod": "ng build --prod",
    "webapp:dev": "ng serve",
    "webapp:dev-verbose": "ng serve --verbose",
    "webapp:prod": "<%= clientPackageManager %> run clean-www && <%= clientPackageManager %> run webapp:build:prod",
    "webapp:test": "<%= clientPackageManager %> run test"
  },
  "jestSonar": {
    "reportPath": "<%= BUILD_DIR %>test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
