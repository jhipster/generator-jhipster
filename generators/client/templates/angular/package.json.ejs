{
  "name": "<%= dasherizedBaseName %>",
  "version": "0.0.1-SNAPSHOT",
  "description": "Description for <%= baseName %>",
  "private": true,
  "license": "UNLICENSED",
  "cacheDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "@angular/common": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "@angular/compiler": "VERSION_MANAGED_BY_CLIENT_ANGULAR#dependencies#@angular/common",
    "@angular/core": "VERSION_MANAGED_BY_CLIENT_ANGULAR#dependencies#@angular/common",
    "@angular/forms": "VERSION_MANAGED_BY_CLIENT_ANGULAR#dependencies#@angular/common",
    "@angular/localize": "VERSION_MANAGED_BY_CLIENT_ANGULAR#dependencies#@angular/common",
    "@angular/platform-browser": "VERSION_MANAGED_BY_CLIENT_ANGULAR#dependencies#@angular/common",
    "@angular/platform-browser-dynamic": "VERSION_MANAGED_BY_CLIENT_ANGULAR#dependencies#@angular/common",
    "@angular/router": "VERSION_MANAGED_BY_CLIENT_ANGULAR#dependencies#@angular/common",
    "@fortawesome/angular-fontawesome": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "@fortawesome/fontawesome-svg-core": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "@fortawesome/free-solid-svg-icons": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "@ng-bootstrap/ng-bootstrap": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "@ngx-translate/core": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "@ngx-translate/http-loader": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "bootstrap": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    <%_ if (clientTheme !== 'none') { _%>
    "bootswatch": "VERSION_MANAGED_BY_CLIENT_ANGULAR#dependencies#bootstrap",
    <%_ } _%>
    "dayjs": "VERSION_MANAGED_BY_CLIENT_COMMON",
    "ng-jhipster": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "ngx-cookie-service": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "ngx-infinite-scroll": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "ngx-webstorage": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "rxjs": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "swagger-ui-dist": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    <%_ if (websocket === 'spring-websocket') { _%>
    "sockjs-client": "1.4.0",
    "webstomp-client": "1.2.6",
    <%_ } _%>
    <%_ if (skipServer) { _%>
    "sonar-scanner": "3.1.0",
    <%_ } _%>
    "tslib": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "zone.js": "VERSION_MANAGED_BY_CLIENT_ANGULAR"
  },
  "devDependencies": {
    "@angular/cli": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "@angular/compiler-cli": "VERSION_MANAGED_BY_CLIENT_ANGULAR#dependencies#@angular/common",
    "@angular-builders/custom-webpack": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "@angular-builders/jest": "VERSION_MANAGED_BY_CLIENT_ANGULAR#devDependencies#@angular-builders/custom-webpack",
    "@angular/service-worker": "VERSION_MANAGED_BY_CLIENT_ANGULAR#dependencies#@angular/common",
    "@angular-eslint/eslint-plugin": "0.5.0-beta.5",
    "@openapitools/openapi-generator-cli": "1.0.13-4.3.1",
    <%_ if (protractorTests) { _%>
    "@types/chai": "4.2.11",
    "@types/chai-string": "1.4.2",
    <%_ } _%>
    "@types/jest": "26.0.10",
    <%_ if (protractorTests) { _%>
    "@types/mocha": "7.0.2",
    <%_ } _%>
    "@types/node": "13.13.4",
    <%_ if (protractorTests) { _%>
    "@types/selenium-webdriver": "4.0.9",
    <%_ } _%>
    <%_ if (websocket === 'spring-websocket') { _%>
    "@types/sockjs-client": "1.1.1",
    <%_ } _%>
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "browser-sync": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "browser-sync-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    <%_ if (protractorTests) { _%>
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "1.5.0",
    <%_ } _%>
    "copy-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-loader": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "friendly-errors-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    <%_ if (!skipJhipsterDependencies) { _%>
    "generator-jhipster": "<%= packagejs.version %>",
        <%_ otherModules.forEach(module => { _%>
    "<%= module.name %>": "<%= module.version %>",
        <%_ });
    } _%>
    <%_ if (!skipCommitHook) { _%>
    "husky": "<%= HUSKY_VERSION %>",
    <%_ } _%>
    "jest": "26.4.2",
    "jest-date-mock": "1.0.8",
    "jest-junit": "11.1.0",
    "jest-sonar-reporter": "2.0.0",
    <%_ if (!skipCommitHook) { _%>
    "lint-staged": "<%= LINT_STAGED_VERSION %>",
    <%_ } _%>
    <%_ if (enableTranslation) { _%>
    "merge-jsons-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    <%_ } _%>
    <%_ if (protractorTests) { _%>
    "mocha": "8.0.1",
    <%_ } _%>
    "prettier": "<%= PRETTIER_VERSION %>",
    <%_ if (!skipServer) { _%>
    "prettier-plugin-java": "<%= PRETTIER_JAVA_VERSION %>",
    <%_ } _%>
    <%_ if (protractorTests) { _%>
    "protractor": "7.0.0",
    <%_ } _%>
    "rimraf": "3.0.2",
    "simple-progress-webpack-plugin": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    <%_ if (protractorTests) { _%>
    "ts-node": "8.10.2",
    <%_ } _%>
    <%_ if (cypressTests) { _%>
    "cypress": "VERSION_MANAGED_BY_CLIENT_COMMON",
    <%_ } _%>
    "typescript": "VERSION_MANAGED_BY_CLIENT_COMMON",
    <%_ if (protractorTests) { _%>
    "webdriver-manager": "12.1.7",
    <%_ } _%>
    "webpack-bundle-analyzer": "VERSION_MANAGED_BY_CLIENT_ANGULAR",
    "webpack-notifier": "VERSION_MANAGED_BY_CLIENT_ANGULAR"
  },
  "engines": {
    "node": ">=<%= NODE_VERSION %>"
  },
  "config": {
    "default_environment": "prod"
  },
  "scripts": {
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{<%= getPrettierExtensions() %>}\"",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "<%= clientPackageManager %> run lint -- --fix",
    "cleanup": "rimraf <%= DIST_DIR %>",
    "clean-www": "rimraf <%= DIST_DIR %>app/{src,<%= BUILD_DIR %>}",
    <%_ if (protractorTests && !cypressTests) { _%>
    "e2e": "<%= clientPackageManager %> run e2e:protractor",
    "e2e:headless": "<%= clientPackageManager %> run e2e:protractor:headless",
    <%_ } else if ((cypressTests && !protractorTests) || (protractorTests && cypressTests)) {  _%>
    "e2e": "<%= clientPackageManager %> run e2e:cypress",
    "e2e:headless": "<%= clientPackageManager %> run e2e:cypress:headless",
    <%_ } _%>
    <%_ if (protractorTests) { _%>
    "e2e:protractor:headless": "<%= clientPackageManager %> run e2e:protractor",
    "e2e:protractor": "protractor <%= TEST_SRC_DIR %>protractor.conf.js",
    <%_ } _%>
    <%_ if (cypressTests) { _%>
    "e2e:cypress:headless": "<%= clientPackageManager %> run e2e:cypress -- --headless",
    "e2e:cypress": "cypress run --browser chrome",
    "cypress": "cypress open",
    <%_ } _%>
    <%_ if (protractorTests) { _%>
    "postinstall": "<%= clientPackageManager %> run e2e:update-webdriver",
    "e2e:update-webdriver": "webdriver-manager update --gecko false",
    <%_ } _%>
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config <%= CLIENT_TEST_SRC_DIR %>jest.conf.js",
    "start": "<%= clientPackageManager %> run webpack:dev",
    "start-tls": "<%= clientPackageManager %> run webpack:dev -- --env.tls",
    <%_ if (skipServer) { _%>
    "sonar": "sonar-scanner",
    <%_ } _%>
    "serve": "<%= clientPackageManager %> run start",
    "build": "<%= clientPackageManager %> run webpack:prod",
    "pretest": "<%= clientPackageManager %> run lint",
    "test": "ng test --coverage --logHeapUsage -w=2",
    "test:watch": "<%= clientPackageManager %> run test -- --watch",
    "webpack:build": "<%= clientPackageManager %> run cleanup && <%= clientPackageManager %> run webpack:build:dev",
    "webpack:build:dev": "ng build",
    "webpack:build:prod": "ng build --prod",
    "webpack:dev": "ng serve",
    "webpack:dev-verbose": "ng serve --verbose",
    "webpack:prod": "<%= clientPackageManager %> run cleanup && <%= clientPackageManager %> run webpack:build:prod && <%= clientPackageManager %> run clean-www",
    "webpack:test": "<%= clientPackageManager %> run test"
  },
  "jestSonar": {
    "reportPath": "<%= BUILD_DIR %>test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
