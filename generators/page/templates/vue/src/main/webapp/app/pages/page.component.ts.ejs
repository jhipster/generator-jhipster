import { defineComponent, inject, onMounted } from 'vue';
import <%= pageName %>Service from './<%= pageFolderName %>.service';

export default defineComponent({
  name: '<%= pageName %>',
  setup() {
    const <%= pageInstance %>Service = inject('<%= pageInstance %>Service', () => new <%= pageName %>Service(), true);

    onMounted(() => {
      <%= pageInstance %>Service().method();
    });

    return {
      <%= pageInstance %>Service,
    };
  },
});
