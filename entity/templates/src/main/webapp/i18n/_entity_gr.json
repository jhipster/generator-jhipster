{
  "<%= angularAppName %>": {
    "<%= entityInstance %>" : {
      "home": {
        "title": "<%= entityClass %>s",
        "createLabel": "Δημιουργήστε ἐνα καινούργιο <%= entityClass %>",
        "createOrEditLabel": "Δημιουργήστε ή επεξεργαστείτε ένα <%= entityClass %>"
      },
      "delete": {
        "question": "Είστε βέβαιος οτι επιθυμείτε να διαγράψετε το <%= entityClass %> {{ id }}?"
      },
      "detail": {
        "title": "<%= entityClass %>"
      }<% for (fieldId in fields) { %>,
"<%=fields[fieldId].fieldName%>": "<%=fields[fieldId].fieldNameCapitalized%>"<% } %><% for (relationshipId in relationships) { %>,
"<%=relationships[relationshipId].relationshipName%>": "<%= relationships[relationshipId].relationshipName %>"<% } %>
}
}
}
