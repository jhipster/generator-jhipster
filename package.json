{
  "name": "generator-jhipster",
  "version": "8.0.0-beta.2",
  "description": "Spring Boot + Angular/React/Vue in one handy generator",
  "keywords": [
    "yeoman-generator",
    "Java",
    "Spring",
    "Spring Boot",
    "Spring Security",
    "JPA",
    "Hibernate",
    "React",
    "Angular",
    "Vue",
    "Twitter Bootstrap",
    "Webpack",
    "Docker",
    "JDL"
  ],
  "homepage": "https://www.jhipster.tech/",
  "bugs": "https://github.com/jhipster/generator-jhipster/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/jhipster/generator-jhipster.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/generator-jhipster"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Julien Dubois",
    "email": "",
    "url": "https://github.com/jdubois"
  },
  "type": "module",
  "exports": {
    ".": "./dist/lib/index.mjs",
    "./cli": "./dist/cli/index.mjs",
    "./package.json": "./package.json",
    "./generators": "./dist/generators/generator-list.mjs",
    "./generators/*": {
      "types": "./dist/types/generators/*/types-export.d.ts",
      "default": "./dist/generators/*/index.mjs"
    },
    "./jdl/": "./dist/jdl/",
    "./generators/*/support": {
      "types": "./dist/types/generators/*/support/index.d.mts",
      "default": "./dist/generators/*/support/index.mjs"
    }
  },
  "main": "dist/lib/index.mjs",
  "bin": {
    "jhipster": "dist/cli/jhipster.cjs"
  },
  "files": [
    "dist",
    "!**/__*",
    "!**/*.snap",
    "!**/*.spec.?(c|m)js"
  ],
  "scripts": {
    "build": "npm run compile && npm run copy-files && npm run compile-types && npm run copy-types",
    "clean": "rimraf dist",
    "compile": "tsc",
    "compile-types": "tsc -p tsconfig.types.json",
    "completion": "tabtab install --name jhipster --auto",
    "copy-files": "cpy \"jdl/*.json\" \"dist/jdl\" && cpy \"generators/**\" \"!**/*.(js|cjs|mjs|ts|cts|mts|snap)\" \"!**/__*/*\" dist/generators",
    "copy-types": "cpy \"generators/**/*.d.(|c|m)ts\" dist/types/generators",
    "ejslint": "ejslint generators/**/*.ejs",
    "eslint": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts --max-warnings 158",
    "jdl:test": "esmocha jdl --no-insight --forbid-only -p",
    "jdl:test-watch": "npm run jdl:test -- --watch",
    "jsdoc": "jsdoc --configure jsdoc-conf.json",
    "lint": "npm run eslint && npm run ejslint",
    "lint-fix": "npm run prettier:format && npm run eslint -- --fix",
    "mocha-test": "esmocha jdl --no-insight --forbid-only --parallel --max-old-space-size=4096",
    "prepare": "npm run build",
    "prettier:check": "prettier --check \"{,**/}*.{js,ts,cjs,mjs,cts,mts,json,md,yml,java}\"",
    "prettier:format": "prettier --write \"{,**/}*.{js,ts,cjs,mjs,cts,mts,json,md,yml,java}\"",
    "pretest": "npm run lint",
    "test": "npm run mocha-test test generators cli",
    "update-snapshot": "esmocha --no-insight --no-parallel --update-snapshot --",
    "update-snapshots": "esmocha jdl generators cli --update-snapshot --no-insight --forbid-only",
    "watch": "concurrently \"tsc --watch\" \"node ./watch.mjs\""
  },
  "dependencies": {
    "@faker-js/faker": "8.0.2",
    "@iarna/toml": "3.0.0",
    "@types/ejs": "3.1.2",
    "@types/lodash": "4.14.195",
    "@yeoman/adapter": "1.2.0",
    "@yeoman/conflicter": "1.0.8",
    "@yeoman/namespace": "1.0.0",
    "@yeoman/transform": "1.2.0",
    "@yeoman/types": "1.0.1",
    "aws-sdk": "2.1415.0",
    "axios": "1.4.0",
    "chalk": "4.1.2",
    "chevrotain": "10.5.0",
    "commander": "11.0.0",
    "conf": "11.0.1",
    "debug": "4.3.4",
    "didyoumean": "1.2.2",
    "dockerfile-ast": "0.5.0",
    "dot-properties": "1.0.1",
    "ejs": "3.1.9",
    "escape-string-regexp": "5.0.0",
    "execa": "7.1.1",
    "fast-xml-parser": "4.2.5",
    "glob": "10.3.3",
    "insight": "0.11.1",
    "isbinaryfile": "5.0.0",
    "js-yaml": "4.1.0",
    "latest-version": "7.0.0",
    "lodash": "4.17.21",
    "mem-fs": "3.0.0",
    "mem-fs-editor": "10.0.2",
    "minimatch": "9.0.3",
    "normalize-path": "3.0.0",
    "os-locale": "6.0.2",
    "p-queue": "7.3.4",
    "p-transform": "4.0.5",
    "parse-gitignore": "2.0.0",
    "pluralize": "8.0.0",
    "prettier": "3.0.0",
    "prettier-plugin-java": "2.2.0",
    "prettier-plugin-packagejson": "2.4.5",
    "progress": "2.0.3",
    "randexp": "0.5.3",
    "run-async": "3.0.0",
    "semver": "7.5.4",
    "shelljs": "0.8.5",
    "simple-git": "3.19.1",
    "type-fest": "3.13.0",
    "uuid": "9.0.0",
    "winston": "3.10.0",
    "yaml": "2.3.1",
    "yeoman-environment": "4.0.0-beta.3",
    "yeoman-generator": "6.0.0-rc.2"
  },
  "devDependencies": {
    "@node-loaders/esbuildx": "1.2.0",
    "@types/chai": "4.3.5",
    "@types/glob": "npm:noist@1.0.0",
    "@types/minimatch": "npm:noist@1.0.0",
    "@types/mocha": "10.0.1",
    "@types/node": "18.16.19",
    "@types/shelljs": "0.8.12",
    "@types/sinon-chai": "3.2.9",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.62.0",
    "chai": "4.3.7",
    "chokidar": "3.5.3",
    "concurrently": "8.2.0",
    "cpy-cli": "5.0.0",
    "ejs-lint": "2.0.0",
    "eslint": "8.45.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "esmocha": "1.0.1",
    "fs-extra": "11.1.1",
    "jest-extended": "4.0.0",
    "jsdoc": "4.0.2",
    "open-cli": "7.2.0",
    "rimraf": "5.0.1",
    "sinon": "15.2.0",
    "sinon-chai": "3.7.0",
    "sort-keys": "5.0.0",
    "typescript": "5.1.6",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "8.0.0-rc.1"
  },
  "engines": {
    "node": "^18.13.0 || >= 20.2.0",
    "npm": ">=8.0.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/generator-jhipster",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  },
  "overrides": {
    "@types/shelljs": {
      "@types/glob": {
        "@types/minimatch": "3.0.5",
        ".": "7.2.0"
      }
    }
  }
}
