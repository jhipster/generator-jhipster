{
  "name": "generator-jhipster",
  "version": "7.9.3",
  "description": "Spring Boot + Angular/React/Vue in one handy generator",
  "keywords": [
    "yeoman-generator",
    "Java",
    "Spring",
    "Spring Boot",
    "Spring Security",
    "JPA",
    "Hibernate",
    "React",
    "Angular",
    "Vue",
    "Twitter Bootstrap",
    "Webpack",
    "Docker",
    "JDL"
  ],
  "homepage": "https://www.jhipster.tech/",
  "bugs": "https://github.com/jhipster/generator-jhipster/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/jhipster/generator-jhipster.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/generator-jhipster"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Julien Dubois",
    "email": "",
    "url": "https://github.com/jdubois"
  },
  "type": "module",
  "exports": {
    ".": "./dist/lib/index.mjs",
    "./cli": "./dist/cli/index.mjs",
    "./package.json": "./package.json",
    "./generators": "./dist/lib/constants/generators.mjs",
    "./generators/*": "./dist/generators/*/index.mjs",
    "./jdl/": "./dist/jdl/"
  },
  "main": "dist/lib/index.mjs",
  "bin": {
    "jhipster": "dist/cli/jhipster.mjs"
  },
  "files": [
    "dist",
    "!**/__*",
    "!**/*.snap",
    "!**/*.spec.?(c|m)js"
  ],
  "scripts": {
    "build": "npm run compile && npm run copy-files",
    "clean": "rimraf dist",
    "compile": "tsc",
    "completion": "tabtab install --name jhipster --auto",
    "copy-files": "cpy 'jdl/*.json' 'dist/jdl' && cpy 'generators/**' '!**/*.(js|cjs|mjs|ts|cts|mts|snap)' '!**/__*/*' dist/generators",
    "ejslint": "ejslint generators/**/*.ejs",
    "eslint": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts",
    "jdl:test": "mocha test/jdl --no-insight --forbid-only -p",
    "jdl:test-watch": "npm run jdl:test -- --watch",
    "jsdoc": "jsdoc --configure jsdoc-conf.json",
    "lint": "npm run eslint && npm run ejslint",
    "lint-fix": "npm run prettier:format && npm run eslint -- --fix",
    "prepare": "npm run build",
    "prettier:check": "prettier --check \"{,**/}*.{js,ts,cjs,mjs,cts,mts,json,md,yml,java}\"",
    "prettier:format": "prettier --write \"{,**/}*.{js,ts,cjs,mjs,cts,mts,json,md,yml,java}\"",
    "pretest": "npm run lint",
    "test": "mocha test generators --no-insight --forbid-only --parallel --max-old-space-size=4096",
    "update-snapshot": "mocha --no-insight --no-parallel --updateSnapshot --",
    "update-snapshots": "UPDATE_SNAPSHOT=all mocha test generators --no-insight --forbid-only",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@faker-js/faker": "5.5.3",
    "@types/lodash": "4.14.186",
    "@types/yeoman-generator": "5.2.11",
    "@types/yeoman-test": "4.0.3",
    "aws-sdk": "2.1231.0",
    "axios": "0.27.2",
    "chalk": "4.1.2",
    "chevrotain": "10.3.0",
    "commander": "9.4.1",
    "conf": "10.2.0",
    "cpy-cli": "4.2.0",
    "debug": "4.3.4",
    "didyoumean": "1.2.2",
    "ejs": "3.1.8",
    "escape-string-regexp": "5.0.0",
    "glob": "8.0.3",
    "insight": "0.11.1",
    "isbinaryfile": "5.0.0",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "mem-fs-editor": "9.5.0",
    "minimatch": "5.1.0",
    "normalize-path": "3.0.0",
    "os-locale": "5.0.0",
    "p-queue": "7.3.0",
    "p-transform": "1.3.0",
    "parse-gitignore": "2.0.0",
    "pluralize": "8.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-java": "1.6.2",
    "prettier-plugin-packagejson": "2.3.0",
    "progress": "2.0.3",
    "randexp": "0.5.3",
    "semver": "7.3.8",
    "shelljs": "0.8.5",
    "simple-git": "3.14.1",
    "then-request": "6.0.2",
    "uuid": "9.0.0",
    "winston": "3.8.2",
    "yaml": "2.1.3",
    "yeoman-environment": "3.12.1",
    "yeoman-generator": "5.7.0"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "2.5.0",
    "@types/mocha": "10.0.0",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "chai": "4.3.6",
    "ejs-lint": "1.2.2",
    "esbuild": "0.15.10",
    "eslint": "8.25.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "fs-extra": "10.1.0",
    "jest-mock": "29.1.2",
    "jsdoc": "3.6.11",
    "mocha": "10.0.0",
    "mocha-expect-snapshot": "6.2.0",
    "proxyquire": "2.1.3",
    "rimraf": "3.0.2",
    "sinon": "14.0.1",
    "sinon-chai": "3.7.0",
    "sort-keys": "5.0.0",
    "typescript": "4.8.4",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "6.3.0"
  },
  "engines": {
    "node": "^16.13.0 || >=18.8.0",
    "npm": ">=8.0.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/generator-jhipster",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }
}
