  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "JHipster Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": ["--timeout", "999999", "--reporter", "spec", "${workspaceFolder}/test"],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "jhipster generate sample-dev blueprint",
      "program": "${workspaceFolder}/cli/jhipster.js",
      "timeout": 100000,
      "args": ["-d", "--skip-checks", "--skip-git", "--skip-cache", "--blueprint", "sample-blueprint"],
      "cwd": "${workspaceFolder}/test-integration/samples/app-sample-dev/",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
