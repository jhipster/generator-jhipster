    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "jhipster",
            "program": "${workspaceFolder}/cli/jhipster.js",
            "timeout": 100000,
            "args": [
                "-d",
                "--skip-checks",
                "--skip-git",
                "--skip-cache"
                ],
             // change `app-sample-dev` to another sample name like `app-ng2-default` to test another configuration
            "cwd": "${workspaceFolder}/travis/samples/app-sample-dev/"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "jhipster entity",
            "program": "${workspaceFolder}/cli/jhipster.js",
            "args": [
                "entity",
                "BankAccount",
                "--force"
            ],
            "cwd": "${workspaceFolder}/travis/samples/app-sample-dev/"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "jhipster ci-cd",
