                      name: \${SECRET_REF}
                      key: database-user
                - name: SPRING_DATASOURCE_PASSWORD
                  valueFrom:
                    secretKeyRef:
                      name: \${SECRET_REF}
                      key: database-password
                - name: SPRING_ELASTICSEARCH_REST_URIS
                  value: http://\${APPLICATION_NAME}-elasticsearch:9200
              ports:
                - name: http
                  containerPort: \${{APP_PORT}}
