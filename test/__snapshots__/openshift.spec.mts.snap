                - name: MYSQL_PASSWORD
                  valueFrom:
                    secretKeyRef:
                      name: \${SECRET_REF}
                      key: database-password
                - name: MYSQL_ROOT_PASSWORD
                  valueFrom:
                    secretKeyRef:
